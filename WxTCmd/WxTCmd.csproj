<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net462;net6.0</TargetFrameworks>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AssemblyTitle>WxTCmd</AssemblyTitle>
    <Company>Eric Zimmerman 501-313-3778</Company>
    <Product>WxTCmd</Product>
    <Description>Windows 10 Timeline parser</Description>
    <Copyright>Copyright ©  2022</Copyright>
    <Version>0.6.6.0</Version>
    <LangVersion>10</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>Calendar Money64.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    
    <PackageReference Include="ServiceStack.OrmLite.Sqlite.Core" Version="5.13.2" />
    
    <PackageReference Include="System.CommandLine" Version="2.0.0-beta2.21616.4" />
    <PackageReference Include="System.CommandLine.NamingConventionBinder" Version="2.0.0-beta2.21616.4" />
    <PackageReference Include="System.IO.Compression" Version="4.*" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.*" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Include="bin\Release\x64\SQLite.Interop.dll" />
    <None Include="bin\Release\x86\SQLite.Interop.dll" />
    <None Include="bin\x64\SQLite.Interop.dll" />
    <None Include="bin\x86\SQLite.Interop.dll" />
    <None Include="bin\Debug\x64\SQLite.Interop.dll" />
    <None Include="bin\Debug\x86\SQLite.Interop.dll" />
    <Content Include="Calendar Money64.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Costura.Fody" Version="5.7.0">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="CsvHelper" Version="27.2.1" />
    <PackageReference Include="Fody" Version="6.6.0">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="GuidMapping" Version="1.0.0.5" />
    <PackageReference Include="NLog" Version="4.7.13" />
    <PackageReference Include="System.Buffers" Version="4.5.1" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.115.5" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.0.0" />
  </ItemGroup>
</Project>